		<bitfield name="PIPE4" low="16" high="18" type="mdp_mixer_stage_id"/>
		<bitfield name="PIPE4_MIXER1" pos="19" type="boolean"/>
		<bitfield name="PIPE5" low="20" high="22" type="mdp_mixer_stage_id"/>
