<import file="copyright.xml"/>

<import file="fifo/nv_object.xml"/>

<domain name="SUBCHAN" bare="yes">
	<stripe prefix="obj-class" variants="NV3_M2MF-G80_M2MF">
		<reg32 offset="0x0180" name="DMA_NOTIFY" type="object"/>
