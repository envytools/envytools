			</reg32>
			<reg32 offset="0x0238" name="OFFSET_IN_HIGH"/>
			<reg32 offset="0x023c" name="OFFSET_OUT_HIGH"/>
		</stripe>
		<reg32 offset="0x030c" name="OFFSET_IN"/>
		<reg32 offset="0x0310" name="OFFSET_OUT"/>
		<reg32 offset="0x0314" name="PITCH_IN" type="int"/>
