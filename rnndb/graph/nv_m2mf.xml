		<stripe variants="G80_M2MF">
			<reg32 offset="0x0200" name="LINEAR_IN" type="boolean"/>
			<reg32 offset="0x0204" name="BLOCKLINEAR_MODE_IN"/>
			<reg32 offset="0x0208" name="BLOCKLINEAR_PITCH_IN" type="int"/>
			<reg32 offset="0x020c" name="BLOCKLINEAR_HEIGHT_IN" type="int"/>
			<reg32 offset="0x0210" name="BLOCKLINEAR_DEPTH_IN" type="int"/> <!-- 1-0x800 -->
			<reg32 offset="0x0214" name="BLOCKLINEAR_POSITION_IN_Z" type="int"/>
			<reg32 offset="0x0218" name="BLOCKLINEAR_POSITION_IN">
				<bitfield name="X" high="15" low="0" type="int"/>
