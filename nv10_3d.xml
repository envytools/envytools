		</stripe>

		<stripe length="2" stride="4">
			<doc> Texture units. </doc>

			<reg32 offset="0x0218" name="TEX_OFFSET"/>
			<reg32 offset="0x0220" name="TEX_FORMAT">
				<bitfield name="DMA0" pos="0" type="boolean"/>
				<bitfield name="DMA1" pos="1" type="boolean"/>
